  "scripts": {
    "dev": "dotenv-checker && vite --host",
    "build": "run-p type-check build-only && yarn run build:min",
    "build:min": "npm exec terser -- -c -m --module -f comments=false -o dist/ui-ressources-gar.min.js -- dist/ui-ressources-gar.js",
    "lint": "eslint src/*",
    "preview": "vite preview",
    "test:unit": "echo No tests available",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "release": "yarn run -T commit-and-tag-version -t 'ui-ressources-gar/v' --path . --release-as $1 || echo 'Please specify a version type (yarn run release -- [patch, minor, major])'"
  },
