    "@types/node": "^22",
    "@uportal/open-id-connect": "^1.40.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.7.7",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
