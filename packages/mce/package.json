{
  "name": "@gip-recia/mce-ui",
  "type": "module",
  "version": "0.0.0",
  "description": "",
  "contributors": [
    {
      "name": "Quentin Guillemin",
      "email": "quentin.guillemin@recia.fr",
      "url": "https://github.com/Quentin-Guillemin"
    }
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/GIP-RECIA/recia-webcomponents/tree/main/packages/mce/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GIP-RECIA/recia-webcomponents.git"
  },
  "bugs": {
    "url": "https://github.com/GIP-RECIA/recia-webcomponents/issues"
  },
  "keywords": [
    "mce",
    "gip-recia"
  ],
  "exports": {
    ".": {
      "import": "./dist/mce-ui.js",
      "require": "./dist/mce-ui.umd.cjs"
    }
  },
  "main": "./dist/mce-ui.umd.cjs",
  "module": "./dist/mce-ui.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "dotenv-checker && vite --host",
    "build": "run-p type-check build-only && npm run build:min",
    "build:min": "npm exec terser -- -c -m --module dist/mce-ui.js > dist/mce-ui.min.js",
    "lint": "eslint src/*",
    "preview": "vite preview",
    "test:unit": "echo No tests available",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "release": "yarn run -T commit-and-tag-version -t 'mce-ui/v'",
    "release:patch": "yarn run -T commit-and-tag-version -t 'mce-ui/v' --release-as patch",
    "release:minor": "yarn run -T commit-and-tag-version -t 'mce-ui/v' --release-as minor",
    "release:major": "yarn run -T commit-and-tag-version -t 'mce-ui/v' --release-as major"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22",
    "@types/regenerator-runtime": "^0",
    "@uportal/open-id-connect": "^1.40.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "axios": "^1.7.9",
    "cropperjs": "^1.6.2",
    "dotenv-checker": "^1.1.7",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.4.3",
    "npm-run-all2": "^7.0.2",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.85.0",
    "terser": "^5.39.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-tsc": "^2.2.2"
  }
}
