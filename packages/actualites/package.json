    "axios": "^1.8.4",
    "dotenv-checker": "^1.1.7",
    "eslint": "^9.26.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.5.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "npm-run-all2": "^8.0.1",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.86.3",
    "terser": "^5.39.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-tsc": "^2.2.10"
  }
