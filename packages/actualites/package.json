  "exports": {
    ".": {
      "import": "./dist/actualites.js"
    }
  },
  "module": "./dist/actualites.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "dotenv-checker && vite --host",
    "build": "run-p type-check build-only && yarn run build:min",
    "build:min": "npm exec terser -- -c -m --module -f comments=false -o dist/actualites.min.js -- dist/actualites.js",
    "lint": "eslint src/*",
    "preview": "vite preview",
    "test:unit": "echo No tests available",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "release": "yarn run -T commit-and-tag-version -t 'actualites/v' --path . --release-as $1 || echo 'Please specify a version type (yarn run release -- [patch, minor, major])'"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
