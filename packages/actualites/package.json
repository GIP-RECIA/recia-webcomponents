    }
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/GIP-RECIA/recia-webcomponents/tree/main/packages/actualites/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GIP-RECIA/recia-webcomponents.git"
  },
  "bugs": {
    "url": "https://github.com/GIP-RECIA/recia-webcomponents/issues"
  },
  "keywords": [
    "actualites",
    "gip-recia"
  ],
  "exports": {
    ".": {
      "import": "./dist/xxx.js",
      "require": "./dist/xxx.umd.cjs"
    }
  },
  "main": "./dist/xxx.umd.cjs",
  "module": "./dist/xxx.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "dotenv-checker && vite --host --port 5173",
    "build": "run-p type-check build-only && npm run build:min",
    "build:min": "npm exec terser -- -c -m --module dist/actualites.js > dist/actualites.min.js",
    "lint": "eslint src/*",
    "preview": "vite preview",
    "test:unit": "echo No tests available",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "release": "yarn run -T commit-and-tag-version -t 'actualites/v'",
    "release:patch": "yarn run -T commit-and-tag-version -t 'actualites/v' --release-as patch",
    "release:minor": "yarn run -T commit-and-tag-version -t 'actualites/v' --release-as minor",
    "release:major": "yarn run -T commit-and-tag-version -t 'actualites/v' --release-as major"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
