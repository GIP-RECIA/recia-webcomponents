{
  "name": "@gip-recia/param-etab",
  "type": "module",
  "version": "0.0.1",
  "description": "UI de paramétrage établissement en VueJS.",
  "files": [
    "dist"
  ],
  "keywords": [
    "gip-recia",
    "param-etab"
  ],
  "homepage": "https://github.com/GIP-RECIA/param-etab-ui/#README.md",
  "bugs": {
    "url": "https://github.com/GIP-RECIA/param-etab-ui/issues"
  },
  "license": "Apache-2.0",
  "author": "Indah Juliani <indah.juliani@recia.fr>",
  "main": "./dist/param-etab.umd.cjs",
  "module": "./dist/param-etab.js",
  "exports": {
    ".": {
      "import": "./dist/param-etab.js",
      "require": "./dist/param-etab.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GIP-RECIA/param-etab-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev\" \"npm run json\"",
    "dev": "vite --host",
    "build": "run-p type-check build-only && npm run build:min",
    "build:min": "npm exec terser -- -c -m --module dist/param-etab.js > dist/param-etab.min.js",
    "preview": "vite preview",
    "json": "json-server --watch ./src/data/api.json",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore --fix src",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@uportal/open-id-connect": "^1.40.2",
    "axios": "^1.6.7",
    "cropperjs": "^1.6.1",
    "regenerator-runtime": "^0.14.0",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue": "^3.3.10",
    "vue-advanced-cropper": "^2.8.8",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^9.8.0",
    "vue-rx": "^6.2.0",
    "vue-sweetalert2": "^5.0.5",
    "vuejs-clipper": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "json-server": "^0.17.4",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.4",
    "terser": "^5.26.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
